{
  "output_dir": "./cdt_results_advanced",
  "seed": 42,
  "device": "cuda:0",
  "num_workers": 4,
  "cache_dir": "./cache",
  
  "pretraining": {
    "enabled": true,
    "dataset_path": "./pretrain_dataset.parquet",
    "treatment_column": "treatment",
    
    "architecture": {
      "embedding_model_name": "all-MiniLM-L6-v2",
      "dragonnet_representation_dim": 128,
      "dragonnet_hidden_outcome_dim": 64,
      "aggregator_mode": "attn",
      "num_latent_confounders": 50,
      "features_per_confounder": 1,
      "explicit_confounder_texts": [
        "What is the patient's level of physical strength/performance status?",
        "What is the patient's smoking history?",
        "What mutations does the tumor have?",
        "What is the tumor's PD-L1 expression?",
        "What are the patient's comorbidities?"
      ],
      "chunk_size": 128,
      "chunk_overlap": 32
    },
    
    "training": {
      "learning_rate": 0.0001,
      "optimizer": "adamw",
      "lr_schedule": "linear",
      "epochs": 10,
      "batch_size": 8,
      "alpha_propensity": 1.0,
      "beta_targreg": 0.01,
      "init_latents_from_kmeans": true
    }
  },
  
  "applied_inference": {
    "dataset_path": "./analysis_dataset.parquet",
    "text_column": "clinical_text",
    "outcome_column": "outcome_indicator",
    "treatment_column": "treatment_indicator",
    "split_column": "split",
    
    "architecture": {
      "embedding_model_name": "all-MiniLM-L6-v2",
      "dragonnet_representation_dim": 128,
      "dragonnet_hidden_outcome_dim": 64,
      "aggregator_mode": "attn",
      "num_latent_confounders": 20,
      "features_per_confounder": 4,
      "explicit_confounder_texts": [
        "What is the patient's level of physical strength/performance status?",
        "What is the patient's smoking history?",
        "What mutations does the tumor have?",
        "What is the tumor's PD-L1 expression?",
        "What are the patient's comorbidities?"
      ],
      "chunk_size": 128,
      "chunk_overlap": 32
    },
    
    "training": {
      "learning_rate": 0.0001,
      "optimizer": "adamw",
      "lr_schedule": "linear",
      "epochs": 50,
      "batch_size": 8,
      "alpha_propensity": 1.0,
      "beta_targreg": 0.1,
      "init_latents_from_kmeans": true
    },
    
    "use_pretrained_weights": true
  },
  
  "plasmode_experiments": {
    "enabled": true,
    "num_repeats": 3,
    
    "generator_architecture": {
      "embedding_model_name": "all-MiniLM-L6-v2",
      "dragonnet_representation_dim": 128,
      "dragonnet_hidden_outcome_dim": 64,
      "aggregator_mode": "attn",
      "num_latent_confounders": 20,
      "features_per_confounder": 4,
      "chunk_size": 128,
      "chunk_overlap": 32
    },
    
    "generator_training": {
      "learning_rate": 0.0001,
      "epochs": 50,
      "batch_size": 8,
      "alpha_propensity": 1.0,
      "beta_targreg": 0.01
    },
    
    "evaluator_architecture": {
      "embedding_model_name": "all-MiniLM-L6-v2",
      "dragonnet_representation_dim": 128,
      "dragonnet_hidden_outcome_dim": 64,
      "aggregator_mode": "attn",
      "num_latent_confounders": 20,
      "features_per_confounder": 4,
      "chunk_size": 128,
      "chunk_overlap": 32
    },
    
    "evaluator_training": {
      "learning_rate": 0.0001,
      "epochs": 50,
      "batch_size": 8,
      "alpha_propensity": 1.0,
      "beta_targreg": 0.1
    },
    
    "plasmode_scenarios": [
      {
        "generation_mode": "phi_linear",
        "preserve_observed_treatments": true,
        "baseline_control_outcome_rate": 0.2,
        "target_ate_logit": 0.5,
        "outcome_heterogeneity_scale": 1.0,
        "ite_heterogeneity_scale": 1.0
      },
      {
        "generation_mode": "phi_nonlinear",
        "preserve_observed_treatments": true,
        "baseline_control_outcome_rate": 0.2,
        "target_ate_logit": 0.5,
        "outcome_heterogeneity_scale": 1.0,
        "ite_heterogeneity_scale": 1.0
      },
      {
        "generation_mode": "deep_nonlinear",
        "preserve_observed_treatments": true,
        "baseline_control_outcome_rate": 0.2,
        "target_ate_logit": 0.5,
        "deep_nonlinear_hidden_dims": [64, 32],
        "deep_nonlinear_dropout": 0.0
      },
      {
        "generation_mode": "gen_y0_sim_uplift",
        "preserve_observed_treatments": true,
        "baseline_control_outcome_rate": 0.5,
        "target_ate_logit": 1.0,
        "uplift_hidden_dims": [64, 32],
        "uplift_activation": "relu",
        "uplift_dropout": 0.0
      }
    ]
  }
}
